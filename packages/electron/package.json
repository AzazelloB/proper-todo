{
  "name": "@proper-todo/electron",
  "version": "0.1.0",
  "private": true,
  "main": "public/index.js",
  "dependencies": {
    "@proper-todo/core": "^0.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron ."
